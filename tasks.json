{
   {
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build calc",
      "type": "shell",
      "command": "gcc",
      "args": [
        "-O2", "-Wall", "-Wextra", "-std=c17", "-g",
        "${workspaceFolder}/calc.c",
        "-o",
        "${workspaceFolder}/calc"
      ],
      "group": { "kind": "build", "isDefault": true },
      "problemMatcher": ["$gcc"]
    }
  ]
}
